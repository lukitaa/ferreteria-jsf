<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <!-- SECTOR DE BEANS -->
    <managed-bean>
        <managed-bean-name>  userLogged     </managed-bean-name>
        <managed-bean-class> entitys.Users  </managed-bean-class>
        <managed-bean-scope> session        </managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>  shoppingCart   </managed-bean-name>
        <managed-bean-class> entitys.ShoppingCart  </managed-bean-class>
        <managed-bean-scope> session        </managed-bean-scope>
    </managed-bean>
    
    <!-- SECTOR DE NAVEGACION -->
    <navigation-rule>
        <!-- se consideran todas las peticiones -->
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>log</from-outcome>
            <to-view-id>principal.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
        
</faces-config>
